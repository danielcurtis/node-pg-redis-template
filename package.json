    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.33",
    "@types/passport": "^1.0.6",
    "@types/passport-google-oauth20": "^2.0.6",
    "@types/pg": "^7.14.11",
    "@types/response-time": "^2.3.4",
    "morgan": "^1.10.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
